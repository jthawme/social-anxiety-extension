{
  "name": "Social Anxiety",
  "description": "Do you need to post?",
  "version": "1.0.3",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["twitter.js"],
      "matches": ["https://*.twitter.com/*"]
    },
    {
      "js": ["instagram.js"],
      "matches": ["https://*.instagram.com/*"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
